{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "maxNodeModuleJsDepth": 5,
    "moduleResolution": "Node",
    "strict": true,
    "target": "ES2022",
    "module": "ESNext",
    "downlevelIteration": true,
    "lib": ["DOM.Iterable", "DOM"]
  },
  "include": ["source"]
}
